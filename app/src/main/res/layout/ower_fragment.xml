<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true">
    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/rl_id"
        android:layout_width="match_parent"
        android:layout_height="550px"
        >
        <MyView.TiltView
            android:id="@+id/sanjiao_id"
            android:layout_width="match_parent"
            android:layout_height="550px"
            android:scaleType="centerCrop"
            android:src="@mipmap/moren3x"
            app:type="1" />
        <ImageView
            android:id="@+id/iv_shezhi_id"
            android:layout_width="80px"
            android:layout_height="80px"
            android:layout_marginLeft="45px"
            android:layout_marginTop="22px"
            android:src="@mipmap/shezhi3x"/>
        <ImageView
            android:id="@+id/company_approve_id"
            android:layout_width="80px"
            android:layout_height="80px"
            android:src="@mipmap/approve3x"
            android:layout_marginTop="22px"
            android:visibility="gone"
            android:layout_alignParentRight="true"
            android:layout_marginRight="45px"
            />
        <de.hdodenhof.circleimageview.CircleImageView
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/head_cv_id"
            android:layout_width="180px"
            android:layout_height="180px"
            android:layout_marginTop="375px"
            android:layout_marginLeft="286px"
            android:layout_marginRight="285px"
            android:src="@mipmap/morentouxiang3x"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            app:civ_border_width="0dp"
            app:civ_border_color="@color/white"/>
    </com.zhy.autolayout.AutoRelativeLayout>
    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/ll_jf_id"
        android:layout_width="match_parent"
        android:layout_height="31px"
        android:orientation="horizontal"
        android:layout_marginTop="25px">
        <ImageView
            android:id="@+id/iv_dj_id"
            android:layout_width="31px"
            android:layout_height="31px"
            android:src="@mipmap/lv1_3x"
            android:layout_marginLeft="279px"/>
        <TextView
            android:layout_width="55px"
            android:layout_height="match_parent"
            android:text="积分:"
            android:textSize="24px"
            android:textColor="#666"
            android:layout_marginLeft="20px"/>
        <TextView
            android:id="@+id/tv_integrals_id"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:textSize="24px"
            android:text="0"
            android:textColor="#666"
            android:layout_marginLeft="20px"/>
    </com.zhy.autolayout.AutoLinearLayout>
    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/all_jcxx_id"
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:layout_marginTop="0px">
        <ImageView
            android:layout_width="44px"
            android:layout_height="44px"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="270px"
            android:src="@mipmap/jichuxinxi3x"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#666666"
            android:textSize="28px"
            android:layout_marginLeft="20px"
            android:layout_gravity="center_vertical"
            android:text="个人信息"/>
    </com.zhy.autolayout.AutoLinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#d9d9d9"
        android:layout_marginLeft="100px"
        android:layout_marginRight="100px"/>
    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/all_wdjf_id"
        android:layout_width="match_parent"
        android:layout_height="100px">
        <ImageView
            android:layout_width="44px"
            android:layout_height="44px"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="270px"
            android:src="@mipmap/wodejifen3x"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#666666"
            android:textSize="28px"
            android:layout_marginLeft="20px"
            android:layout_gravity="center_vertical"
            android:text="积分明细"/>
    </com.zhy.autolayout.AutoLinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#d9d9d9"
        android:layout_marginLeft="100px"
        android:layout_marginRight="100px"/>
    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/all_wdsc_id"
        android:layout_width="match_parent"
        android:layout_height="100px"
        >
        <ImageView
            android:layout_width="44px"
            android:layout_height="44px"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="270px"
            android:src="@mipmap/wodeshoucang3x"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#666666"
            android:textSize="28px"
            android:layout_marginLeft="20px"
            android:layout_gravity="center_vertical"
            android:text="我的收藏"/>
    </com.zhy.autolayout.AutoLinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#d9d9d9"
        android:layout_marginLeft="100px"
        android:layout_marginRight="100px"/>
    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/all_wdfb_id"
        android:layout_width="match_parent"
        android:layout_height="100px"
       >
        <ImageView
            android:layout_width="44px"
            android:layout_height="44px"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="270px"
            android:src="@mipmap/wodefabu3x"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#666666"
            android:textSize="28px"
            android:layout_marginLeft="20px"
            android:layout_gravity="center_vertical"
            android:text="我的发布"/>
    </com.zhy.autolayout.AutoLinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#d9d9d9"
        android:layout_marginLeft="100px"
        android:layout_marginRight="100px"/>
    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/all_wdcg_id"
        android:layout_width="match_parent"
        android:layout_height="100px"
        >
        <ImageView
            android:layout_width="44px"
            android:layout_height="44px"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="270px"
            android:src="@mipmap/wodecaogao3x"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#666666"
            android:textSize="28px"
            android:layout_marginLeft="20px"
            android:layout_gravity="center_vertical"
            android:text="我的草稿"/>
    </com.zhy.autolayout.AutoLinearLayout>
</com.zhy.autolayout.AutoLinearLayout>