<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="select_btn" >
        <item name="android:layout_width">0px</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginLeft">2px</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">#333</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
    </style>
    <style name="CircleImageView_yuan">
        <item name="android:layout_width">80px</item>
        <item name="android:layout_height">80px</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">10px</item>
        <item name="android:src">@mipmap/yuan3x</item>
    </style>
    <style name="zwd_style">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:button">@null</item>
    </style>
    <style name="vertical_fenge_style">
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">#DCDCDC</item>
    </style>
    <style name="horizontal_fenge_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2px</item>
        <item name="android:background">#DCDCDC</item>
    </style>
</resources>